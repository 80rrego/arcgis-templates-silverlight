        <GradientStop Color="#FFE7EEEF" Offset="1" />
    </LinearGradientBrush>

    <Style x:Key="PanelBorder" TargetType="Border" >
        <Setter Property="BorderBrush" Value="Gray" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="4" />
