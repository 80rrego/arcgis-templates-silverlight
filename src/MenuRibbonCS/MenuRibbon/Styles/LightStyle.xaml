    <RadialGradientBrush x:Key="ButtonGradient" >
        <GradientStop Color="#FF739AC2" Offset="0"/>
        <GradientStop Color="#FF2A4F77" Offset="1"/>
    </RadialGradientBrush>

    <LinearGradientBrush x:Key="CommonGradient" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="#ee76a8d3"/>
        <GradientStop Offset="0.25" Color="#ee5b8cb5"/>
        <GradientStop Offset="0.75" Color="#ee4b7ba7"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="CommonBorderBrush" Color="#66000000" />
    <SolidColorBrush x:Key="CommonPanelBorderBackgroundBrush" Color="#44FFFFFF" />
    <SolidColorBrush x:Key="BaseColor" Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="AppBackground" Color="#ee4b7ba7" />

    <LinearGradientBrush x:Key="RibbonButtonHighlight" StartPoint="0.5,0" EndPoint="0.5,1" >
        <GradientStop Color="#FFFFFDF3" Offset="0" />
        <GradientStop Color="#FFFFEBA5" Offset="0.5" />
        <GradientStop Color="#FFFFEBA5" Offset="0.5" />
        <GradientStop Color="#FFFFE794" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PanelGradient" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.75">
